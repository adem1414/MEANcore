{
  "name": "meancore",
  "description": "Full-Stack JavaScript  with MongoDB, Express, AngularJS, and Node.js.",
  "version": "0.5.0-beta",
  "private": false,
  "author": "david.oravsky",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/meancore/meancore.git"
